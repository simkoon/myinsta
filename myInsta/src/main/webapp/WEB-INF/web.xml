<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetContents</servlet-name>
  	<display-name>GetContents</display-name>
  	<description></description>
  	<servlet-class>com.koreait.GetContents</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LikeServlet</servlet-name>
  	<display-name>LikeServlet</display-name>
  	<description></description>
  	<servlet-class>com.koreait.LikeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveServlet</servlet-name>
  	<display-name>SaveServlet</display-name>
  	<description></description>
  	<servlet-class>com.koreait.SaveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetContents</servlet-name>
  	<url-pattern>/GetContents</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LikeServlet</servlet-name>
  	<url-pattern>/LikeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveServlet</servlet-name>
  	<url-pattern>/SaveServlet</url-pattern>
  </servlet-mapping>
</web-app>
